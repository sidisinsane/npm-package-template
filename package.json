{
  "name": "npm-package-template",
  "version": "0.0.1",
  "description": "A npm package template with typescript.",
  "keywords": [
    "npm",
    "boilerplate",
    "template",
    "typescript",
    "tsup",
    "prettier",
    "eslint",
    "husky",
    "lint-staged",
    "commitizen",
    "semantic-release"
  ],
  "homepage": "https://github.com/sidisinsane/npm-package-template#readme",
  "bugs": {
    "url": "https://github.com/sidisinsane/npm-package-template/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sidisinsane/npm-package-template.git"
  },
  "license": "MIT",
  "author": {
    "name": "Dirk Sidney Jansen",
    "email": "sidisinsane@users.noreply.github.com",
    "url": "https://github.com/sidisinsane"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "cz": "cz",
    "docs": "typedoc",
    "format": "prettier . --check",
    "format:fix": "prettier . --write",
    "postinstall": "pnpm run build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:staged": "lint-staged",
    "prepare": "husky install",
    "release": "semantic-release",
    "test": "jest"
  },
  "devDependencies": {
    "@mxssfd/typedoc-theme": "^1.1.3",
    "@types/jest": "^29.5.4",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "conventional-changelog-conventionalcommits": "^7.0.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.6.4",
    "prettier": "3.0.3",
    "semantic-release": "^21.1.1",
    "ts-jest": "^29.1.1",
    "tsup": "^7.2.0",
    "typedoc": "^0.25.1",
    "typescript": "^5.2.2"
  }
}
